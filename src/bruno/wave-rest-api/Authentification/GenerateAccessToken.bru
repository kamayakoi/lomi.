meta {
  name: Generate Access Token
  type: http
  seq: 1
}

post {
  url: {{base_url}}/v1/oauth/token
  body: json
  auth: none
}

headers {
  Content-Type: application/json
}

body:json {
  {
    "client_id": "{{client_id}}",
    "client_secret": "{{client_secret}}",
    "grant_type": "client_credentials"
  }
}

docs {
  Generates an access token for authentication.
}