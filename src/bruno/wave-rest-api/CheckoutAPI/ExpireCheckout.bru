meta {
  name: Expire Checkout
  type: http
  seq: 2
}

post {
  url: {{base_url}}/v1/checkout/sessions/:id/expire
  auth: bearer
}

headers {
  Authorization: Bearer {{access_token}}
}

docs {
  Expires an open Checkout Session, preventing it from being completed by any user.

  Parameters:
  No parameters.

  Returns:
  Returns an empty body and an HTTP 200 Success code when successfully expiring the session.
}